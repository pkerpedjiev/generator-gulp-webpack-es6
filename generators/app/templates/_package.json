  "keywords": [
    "yeoman-generator",
    "babel",
    "browser-sync",
    "ES6",
    "webpack-dev-server",
    "d3",
    "webpack",
    "webpack2"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pkerpedjiev/generator-webpack-es6"
  },
  "engines": {
    "node": ">=0.12.0"
