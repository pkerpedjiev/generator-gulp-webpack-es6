  "devDependencies": {
    "babel-core": "~6.5.2",
    "babel-loader": "~6.2.3",
    "babel-preset-es2015": "~6.5.0",
    "browser-sync": "~2.11.1",
    "del": "~2.2.0",
    "es6-promise": "~3.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "~3.1.0",
    "gulp-babel": "~6.1.2",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.1.1",
    "gulp-debug": "^2.1.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-load-plugins": "~1.2.0",
    "gulp-plumber": "~1.1.0",
    "gulp-shell": "^0.5.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.0.7",
    "gulp-webpack": "~1.5.0",
    "main-bower-files": "~2.11.1",
    "webpack": "latest",
    "webpack-dev-middleware": "git+https://github.com/webpack/webpack-dev-middleware.git",
    "webpack-dev-server": "latest",
    "webpack-stream": "latest",
    "wiredep": "latest"
  },
